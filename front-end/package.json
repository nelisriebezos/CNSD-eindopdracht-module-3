{
  "name": "cnsdm-eindopdracht-3",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "run-p build preview-only",
    "preview-only": "vite preview",
    "test:unit": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:coverage:ui": "vitest --ui",
    "test:e2e": "NODE_ENV='e2e' start-server-and-test preview http://localhost:4173 'cypress run --e2e --browser chromium --headless'",
    "test:start-server": " start-server-and-test preview http://localhost:4173",
    "test:e2e-windows": "set NODE_ENV=e2e && start-server-and-test preview http://localhost:4173 'cypress run --e2e --browser chrome --headless'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e --browser chromium'",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force"
  },
  "dependencies": {
    "@shoelace-style/shoelace": "^2.12.0",
    "pinia": "^2.1.7",
    "uuid": "^9.0.1",
    "vue": "^3.3.11",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@tsconfig/node18": "^18.2.2",
    "@types/jest": "^29.5.11",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^18.19.3",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vitest/coverage-v8": "^1.1.3",
    "@vitest/ui": "^1.1.3",
    "@vue/test-utils": "^2.4.3",
    "@vue/tsconfig": "^0.5.0",
    "cypress": "^13.6.1",
    "jsdom": "^23.0.1",
    "npm-run-all2": "^6.1.1",
    "sass": "^1.69.5",
    "start-server-and-test": "^2.0.3",
    "typescript": "~5.3.0",
    "vite": "^5.0.10",
    "vitest": "^1.0.4",
    "vue-tsc": "^1.8.25"
  }
}
